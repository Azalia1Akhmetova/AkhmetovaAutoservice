<Window x:Class="AkhmetovaAutoservice.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AkhmetovaAutoservice"
        mc:Ignorable="d"
        Title="Автосервис Ахметова" Height="450" Width="800" FontFamily="Tw Cen MT" Icon="Resources/service_logo.png">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="149*"/>
            <ColumnDefinition Width="651*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="35"/>
        </Grid.RowDefinitions>
        <Image Source="Resources/service_logo.png" HorizontalAlignment="Left"></Image>
        <TextBlock Text ="Услуги автосервиса" FontSize="30" HorizontalAlignment="Left" VerticalAlignment="Center" Grid.Column="1" Margin="116,0,0,0"></TextBlock>
        <Frame Grid.Row="1" Name="MainFrame" ContentRendered="MainFrame_ContentRendered" NavigationUIVisibility="Hidden" Grid.ColumnSpan="2"></Frame>
        <Button Content="Назад" Name="BtnBack" HorizontalAlignment="Left"
                Click="BtnBack_Click" Grid.Column="1" Margin="471,22,0,23"></Button>
    </Grid>
</Window>
